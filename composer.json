{
	"name": "wppunk/subscribe",
	"license": "MIT",
	"authors": [
		{
			"name": "WPPunk",
			"email": "i@wp-punk.com"
		}
	],
	"config": {
		"platform": {
			"php": "7.4"
		}
	},
	"minimum-stability": "stable",
	"require": {},
	"autoload": {
		"psr-4": {
			"Subscribe\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Subscribe\\": "src",
			"Subscribe\\Tests\\": ".tests/php/unit",
			"Subscribe\\Tests\\WPUnit\\": ".tests/php/wpunit"
		}
	},
	"require-dev": {
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.3",
		"codeception/module-db": "^1.1",
		"codeception/module-webdriver": "^1.1",
		"lucatume/wp-browser": "^3.0",
		"lucatume/function-mocker": "^1.3",
		"10up/wp_mock": "~0.4"
	},
	"scripts": {
		"acceptance": "codecept run acceptance -c .codeception.yml",
		"integration": "codecept run wpunit -c .codeception.yml",
		"unit": "codecept run unit -c .codeception.yml"
	}
}
